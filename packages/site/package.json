    "@antv/util": "^2.0.9",
    "@turf/turf": "^6.5.0",
    "antd": "^4.20.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-transform-import-css-l7": "^0.0.6",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^7.0.3",
    "dumi": "^2.2.1",
    "gcoord": "^0.3.2",
    "geotiff": "^2.0.7",
    "gh-pages": "^2.1.1",
    "lerc": "^3.0.0",
    "pmtiles": "^2.7.2",
    "popmotion": "^11.0.5",
    "read-markdown": "^1.0.2",
