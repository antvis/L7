{
  "name": "@antv/l7",
  "version": "2.21.1",
  "description": "A Large-scale WebGL-powered Geospatial Data Visualization",
  "repository": "git@github.com:antvis/L7.git",
  "license": "MIT",
  "author": "https://github.com/orgs/antvis/people",
  "sideEffects": true,
  "main": "lib/index.js",
  "unpkg": "dist/l7.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "README.md"
  ],
  "scripts": {
    "build": "father build",
    "build-dev": "COMPRESS=none father build",
    "build:cjs": "BABEL_ENV=cjs babel src --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start --no-comments",
    "build:esm": "BABEL_ENV=esm babel src --root-mode upward --out-dir es --source-maps --extensions .ts,.tsx --delete-dir-on-start --no-comments",
    "clean": "rimraf dist; rimraf es; rimraf lib;",
    "sync": "tnpm sync",
    "tsc": "tsc --project tsconfig.build.json",
    "watch": "BABEL_ENV=cjs babel src --watch --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start --no-comments"
  },
  "dependencies": {
    "@antv/l7-component": "2.21.1",
    "@antv/l7-core": "2.21.1",
    "@antv/l7-layers": "2.21.1",
    "@antv/l7-maps": "2.21.1",
    "@antv/l7-scene": "2.21.1",
    "@antv/l7-source": "2.21.1",
    "@antv/l7-utils": "2.21.1",
    "@babel/runtime": "^7.7.7",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "edc5dca4af25650507d1b3b3a649bf6c0cb6089d"
}
