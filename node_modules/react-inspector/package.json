    "eslint-plugin-react": "^7.7.0",
    "expect": "^24.0.0",
    "jest": "^24.0.0",
    "prettier": "^1.16.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hot-loader": "^4.0.1",
