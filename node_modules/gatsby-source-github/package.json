{
  "name": "gatsby-source-github",
  "description":
    "A gatsby source plugin to source Github's GraphQL API for build-time data for Gatsby",
  "version": "0.0.2",
  "main": "index.js",
  "author": "Dustin Schau <dustinschau@gmail.com> (https://dustinschau.com)",
  "repository": "https://github.com/DSchau/gatsby-source-github.git",
  "license": "MIT",
  "keywords": ["gatsby", "gatsby-plugin", "gatsby-source-plugin"],
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-plugin-transform-object-rest-spread": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "cross-env": "~5.1.4",
    "husky": "~0.14.3",
    "jest": "~22.4.3",
    "prettier": "~1.11.1",
    "pretty-quick": "~1.4.1"
  },
  "scripts": {
    "precommit": "pretty-quick --staged",
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "test": "jest",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  },
  "dependencies": {
    "graphql-request": "~1.5.1",
    "lodash": "~4.17.5",
    "yup": "~0.24.1"
  }
}
